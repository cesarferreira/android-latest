allprojects {
  ext {
    latestAndroidVersionNumber = "<%= hash['latestAndroidVersionNumber'] %>"
    latestAndroidVersionName = "<%= hash['latestAndroidVersionName'] %>"
    latestAndroidReleaseDate = "<%= hash['latestAndroidReleaseDate'] %>"
    latestAndroidVersionCode = <%= hash['latestAndroidVersionCode'] %>
    compileSdkVersion = <%= hash['compileSdkVersion'] %>
    buildToolsVersion = "<%= hash['buildToolsVersion'] %>"
    googlePlayServicesVersion = "<%= hash['googlePlayServicesVersion'] %>"
    targetSdkVersion = <%= hash['targetSdkVersion'] %>
    latestSupportVersion = "<%= hash['latestSupportVersion'] %>"
    buildGradleToolsVersion = "<%= hash['buildGradleToolsVersion'] %>"
  }
}
